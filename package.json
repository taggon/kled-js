{
  "name": "kled",
  "version": "0.1.5",
  "type": "module",
  "module": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "keywords": [
    "fuzzy",
    "string",
    "search",
    "levenshtein",
    "distance",
    "similarity",
    "korean"
  ],
  "author": {
    "name": "Taegon Kim",
    "url": "https://taegon.kim",
    "email": "gonom9@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/taggon/kled-js.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "bun test",
    "coverage": "bun test --coverage",
    "prepare": "bun run build",
    "version": "auto-changelog -p --starting-version v0.1.4 && git add CHANGELOG.md"
  },
  "files": [
    "dist",
    "docs"
  ],
  "devDependencies": {
    "auto-changelog": "^2.4.0",
    "bun-types": "latest",
    "typescript": "^5.2.2"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": false,
    "commitLimit": false,
    "backfillLimit": false,
    "hideCredit": true
  }
}
